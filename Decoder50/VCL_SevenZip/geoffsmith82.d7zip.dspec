  {
   "name": "default",
   "source": [
    {
     "src": "*.pas",
     "dest": "src\\",
     "exclude": [
      "*.dcu"
     ]
    }
   ],
   "searchPaths": [
    {
     "path": "src"
    }
   ],
   "lib": [
    {
     "src": "bin$bitness64Only$\\*.*",
     "dest": "bin$bitness64Only$"
    }
   ]
  }
